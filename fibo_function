#function to print a fibo series until a given term:
def fibo(num):
    if num == 1:
        return 0
    else: 
        num1 = 0
        num2 = 1
        factlist = [0,1]
        for i in range(1,num-1):
            num3 = num1 + num2
            num1 = num2
            num2 = num3
            factlist.append(num3)
        return factlist
