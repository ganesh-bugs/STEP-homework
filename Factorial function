#Factorial using method 1:
def factorial(num):
    fact = 1
    while num != 1:
        fact = fact * num
        num -= 1
    return fact
    
 #Factorial using recursion:
 def fact(num):
    factorial = 1
    if num == 1:
        return factorial
    else:
        factorial = num * fact(num-1)
        return factorial
